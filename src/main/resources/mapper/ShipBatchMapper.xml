<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cienet.shipment.repository.ShipBatchRepo">

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_by, created_date, last_modified_date,last_modified_by
    </sql>

    <select id="getShipBatchById" resultType="com.cienet.shipment.vo.ShipBatchQueryVo">
        select
        <include refid="Base_Column_List"/>
        from ship_batch where id = #{id}
    </select>

    <select id="getShipBatchPageList" resultType="com.cienet.shipment.vo.ShipBatchQueryVo">
        select
        <include refid="Base_Column_List"/>
        from ship_batch
    </select>

</mapper>
